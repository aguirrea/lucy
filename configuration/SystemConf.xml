        <Name>CrossoverRate</Name>
        <Value>1.0</Value>
    </Property>
    <Property>
        <Name>MutationRate</Name>
        <!--value between 0 and 0.1, 0.02 represents 0.2 %-->
        <Value>0.05</Value>
    </Property>
